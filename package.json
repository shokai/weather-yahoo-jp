{
  "name": "weather-yahoo-jp",
  "version": "0.0.1",
  "private": true,
  "description": "get weather info from yahoo.co.jp",
  "main": "lib/index.js",
  "engines": {
    "node": ">= 5.2.0",
    "npm": "3.x"
  },
  "scripts": {
    "mocha": "mocha test/test_*.es6 --compilers js:babel-register",
    "eslint": "eslint *.es6 src/*.es6 test/*.es6",
    "test": "npm run eslint && npm run mocha",
    "build": "babel src/ --out-dir lib/ --source-maps inline",
    "watch": "parallelshell 'npm run build -- --watch' 'chokidar \"(src|test)/*.es6\" -c \"npm run eslint\"'"
  },
  "keywords": [
    "weather",
    "forecast",
    "yahoo"
  ],
  "author": "Sho Hashimoto <hashimoto@shokai.org>",
  "license": "MIT",
  "dependencies": {
    "debug": "^2.2.0",
    "lodash": "^4.0.0",
    "superagent": "^1.6.1"
  },
  "devDependencies": {
    "babel-cli": "^6.4.0",
    "babel-eslint": "^4.1.6",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-stage-3": "^6.3.13",
    "chokidar-cli": "^1.2.0",
    "eslint": "^1.10.3",
    "eslint-plugin-if-in-test": "^0.1.2",
    "parallelshell": "^2.0.0"
  }
}
